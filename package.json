  "main": "build/index.umd.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "default": "./build/index.umd.js"
      },
      "require": "./build/index.cjs",
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "default": "./build/index.js"
    }
  },
  "files": [
    "build"
  ],
  "pre-commit": [
    "style",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmanuelHickle/voluptatumut.git"
  },
  "keywords": [
    "et",
    "nihil",
    "non",
    "pariatur",
    "unde",
    "create",
    "deserunt",
    "hic",
    "et",
    "officia"
  ],
